ht: 100%;\n  width: 100%;\n  display: flex;\n  align-items: center;\n}\n\n.search {\n  display: flex;\n  width: 100%;\n\n  input {\n    flex: 1;\n    background: transparent;\n    outline: none;\n    font-size: 18px;\n    margin-right: 0.5rem;\n    padding: 0.35rem 0.5rem 0.35rem 0.25rem;\n    border: none {\n      bottom: 2px solid $color-primary-100;\n    }\n\n    &:focus {\n      border-bottom: 2px solid $color-primary-300;\n    }\n  }\n\n  button {\n    padding: 0.75rem;\n    background: $color-primary-200;\n    border-radius: 0.35rem;\n    color: #fff;\n    font-weight: bold;\n    cursor: pointer;\n    border: none {\n      bottom: 1px solid $color-primary-200;\n    }\n\n    &:hover {\n      background: $color-primary-300;\n    }\n  }\n}\n","src/sass/_cards.scss":".cards--section {\n  &__layout {\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n    grid-gap: 1rem;\n    padding-right: 1rem;\n    margin-bottom: 2rem;\n    @media screen and (max-width: $bp-medium) {\n      padding-right: 0rem;\n    }\n    @media screen and (max-width: $bp-small) {\n      grid-template-columns: 1fr;\n    }\n  }\n}\n\n.message {\n  grid-column: 1 / 3;\n\n  display: flex;\n  align-items: center;\n  padding: 2.5rem 1.5rem;\n  border-radius: $border-radius;\n  background: $color-secondary-300;\n  box-shadow: $box-shadow-sm;\n\n  &__content {\n    font-size: 20px;\n  }\n\n  i {\n    font-size: 2rem;\n    margin-right: 1rem;\n    padding-right: 1rem;\n    color: $color-primary-400;\n    border-right: 1px solid $color-primary-100;\n  }\n}\n\n.placecard {\n  cursor: pointer;\n  padding: 2rem;\n  background: $color-secondary-300;\n  border: 2px solid $color-secondary-300;\n  box-shadow: $box-shadow-sm;\n  border-radius: $border-radius;\n\n  &:hover {\n    background: $color-secondary-200;\n  }\n}\n\n.spinner {\n  &__bg {\n    padding: 1rem;\n    background: $color-secondary-300;\n    border-radius: 50%;\n    box-shadow: $box-shadow-lg;\n\n    .spin {\n      font-size: 2rem;\n      color: $color-primary-400;\n      animation-duration: 2s;\n      animation-name: spinning;\n      animation-iteration-count: infinite;\n    }\n  }\n}\n\n/* Used for the spinner and to center it on the grid */\n.card__layout--middle {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  grid-column: 1 / 3;\n  height: calc(100vh - 170px);\n}\n\n@keyframes spinning {\n  from {\n    transform: rotate(0deg);\n  }\n\n  to {\n    transform: rotate(360deg);\n  }\n}\n\n.card {\n  background: #fff;\n  box-shadow: $box-shadow-sm;\n  display: flex;\n  flex-direction: column;\n  border: 5px solid transparent;\n\n  &.highlighted {\n    border: 5px solid $color-primary-100;\n  }\n\n  &--image {\n    width: 100%;\n    height: 250px;\n    background-position: center; /* Center the image */\n    background-repeat: no-repeat; /* Do not repeat the image */\n    background-size: cover;\n  }\n\n  &--body {\n    padding: 1rem;\n    display: flex;\n    flex-direction: column;\n    flex: 1;\n\n    &__title {\n      font-size: 1.2rem;\n      margin-bottom: 1rem;\n    }\n\n    .card--details {\n      margin-top: auto;\n\n      &__price {\n        margin-bottom: 0.75rem;\n      }\n\n      &__icons {\n        display: flex;\n        justify-content: space-between;\n\n        .card--icon {\n          font-size: 0.85rem;\n          .fas {\n            color: $color-primary-300;\n            margin-right: 0.3rem;\n          }\n        }\n\n        .card--bookmark {\n          color: $color-primary-300;\n          font-size: 1.35rem;\n        }\n      }\n    }\n  }\n}\n","src/sass/_map.scss":".map {\n  overflow: hidden;\n\n  &--fixed {\n    position: fixed;\n\n    &__wrapper {\n      height: 100%;\n      width: 100%;\n      position: relative;\n    }\n  }\n\n  #mapid {\n    width: 34vw;\n    height: calc(100vh - 60px * 2 - 0.75rem);\n    overflow: hidden;\n\n    @media screen and (min-width: 1251px) {\n      width: calc((1250px / 3) - 0.5rem);\n    }\n  }\n}\n\n@media screen and (max-width: $bp-medium) {\n  .map {\n    display: none;\n    visibility: none;\n  }\n}\n\n.custom-popup {\n  cursor: pointer;\n\n  .leaflet-popup-tip {\n    background: $color-primary-100;\n  }\n\n  &:hover {\n    z-index: 1000;\n\n    .leaflet-popup-content-wrapper {\n      background: $color-primary-300;\n    }\n\n    .leaflet-popup-tip {\n      background: $color-primary-300;\n    }\n  }\n\n  .leaflet-popup-content-wrapper {\n    background: $color-primary-100;\n\n    .leaflet-popup-content {\n      text-align: center;\n      color: #fff;\n      font-size: 0.75rem;\n      margin: 0.5rem;\n\n      .popup__content {\n        padding: 0.5rem;\n      }\n    }\n  }\n}\n\n.leaflet-popup {\n  .leaflet-popup-tip-container {\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"315b837337a981bfbb4512d75d22e920","cacheData":{"env":{}}}